 2 files changed, 7 insertions(+), 4 deletions(-)

diff --git a/calculator.cpp b/calculator.cpp
index d93e35b..4fe398b 100644
--- a/calculator.cpp
+++ b/calculator.cpp
@@ -2,10 +2,13 @@
 
 int Calculator::Add (double a, double b)
 {
+<<<<<<< HEAD
 	return a + b + 0.5;
+=======
+>>>>>>> cbcb06d (formatting: use tabs instead of spaces)
 }
 
